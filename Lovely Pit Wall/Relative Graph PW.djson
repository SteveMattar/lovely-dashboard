                  "Formula": {
                    "Expression": "if ( $prop('variable.graphMode') == 'overall' ) {\r\n\tdriverBehindDistance = $prop('PersistantTrackerPlugin.DriverBehind_00_Distance');\r\n\tdriverAheadDistance = $prop('PersistantTrackerPlugin.DriverAhead_00_Distance');\r\n} else {\r\n\tdriverBehindDistance = driverrelativedistancetoplayer(getopponentleaderboardposition_aheadbehind_playerclassonly(1));\r\n\tdriverAheadDistance = driverrelativedistancetoplayer(getopponentleaderboardposition_aheadbehind_playerclassonly(-1));\r\n}\r\n\r\nbehindDistance = Math.round( (driverBehindDistance / (driverBehindDistance + Math.abs(driverAheadDistance))) * 100 )\r\n\r\nif ( ld_isIncreasing(behindDistance) == true ) {\r\n\treturn 'icon-right-green'\r\n} else if ( ld_isIncreasing( behindDistance) == false ) {\t\r\n\treturn 'icon-left-red'\r\n} else { \r\n\treturn 'icon-right-green'\r\n}",
