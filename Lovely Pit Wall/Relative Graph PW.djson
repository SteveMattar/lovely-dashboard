                  },
                  "Mode": 2
                },
                "Left": {
                  "Formula": {
                    "Expression": "if ( $prop('variable.graphMode') == 'overall' ) {\r\n\ttotalDistance = $prop('PersistantTrackerPlugin.DriverBehind_00_Distance') + Math.abs($prop('PersistantTrackerPlugin.DriverAhead_00_Distance'))\r\n\tpercentPosition = ($prop('PersistantTrackerPlugin.DriverBehind_00_Distance') / totalDistance) * 100\r\n} else {\r\n\ttotalDistance = Math.abs(driverrelativedistancetoplayer(getopponentleaderboardposition_aheadbehind_playerclassonly(1))) + Math.abs(driverrelativedistancetoplayer(getopponentleaderboardposition_aheadbehind_playerclassonly(-1)))\r\n\tpercentPosition = (Math.abs(driverrelativedistancetoplayer(getopponentleaderboardposition_aheadbehind_playerclassonly(1))) / totalDistance) * 100\r\n}\r\n\r\nreturn Math.round( (180 * (percentPosition / 100)) + 444 )\r\n\r\n// 444 - 620\r\n//percentPosition",
